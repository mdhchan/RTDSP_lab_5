void ISR_AIC(void){
	//implements IIR Filter direct form 2 with single buffer
	//w[index] is the memory array
	int index;
	double y;
	w[0] = (double)mono_read_16Bit();	
	//not sure how to implement b0
	for (index = N; index > 0; index --){
		y += (b[index]-a[index]*b[0])*w[index];
		w[index] = w[index-1];
	}	
	y += w[0]*b[0];
	mono_write_16Bit((Int16)y);
}
