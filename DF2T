void ISR_AIC(void){
	//implements IIR Filter direct form 2 transposed with single buffer
	//x[index] is the memory array
	int index;
	double y;
	x[0] = (double)mono_read_16Bit();
	y = b[0]*x_0 + x[1];
	mono_write_16Bit((Int16)y);
	for (index = 1, index < N; index ++){
		x[index] = b[index]*x[0] - a[index]*y + x[index+1];
	}
	x[N] = b[N]*x[0] - a[N]*y;
}
