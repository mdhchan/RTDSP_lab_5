void ISR_AIC(void){
	//implements IIR Filter direct form 2 transposed with single buffer
	//w[index] is the memory array N is 5 for fourth order filter
	int index;
	double y = 0;
	double x = (double)mono_read_16Bit();
	y = b[0]*x + w[1];
	mono_write_16Bit((Int16)y);
	for (index = 1; index < N-1; index ++){
		w[index] = b[index]*x - a[index]*y + w[index+1];
	}
	w[N-1] = b[N]*x - a[N]*y;
}
