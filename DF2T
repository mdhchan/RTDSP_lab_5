void ISR_AIC(void){
	//implements IIR Filter direct form 2 transposed with single buffer
	//x[index] is the memory array
	int index;
	double y;
	w[0] = (double)mono_read_16Bit();
	y = b[0]*w[0] + w[1];
	mono_write_16Bit((Int16)y);
	for (index = N - 1; index >0; index --){
		w[index] = b[index]*w[0] - a[index]*y + w[index+1];
	}
	w[N] = b[N]*w[0] - a[N]*y;
}
